#!/bin/sh
set -e

TF=$1
# Load natural timeframes
mysqlimport --ignore --local --fields-terminated-by='\t' --lines-terminated-by='\n' -s -ufxcm fxcm *_$TF
rm -Rf *_$TF

# Create synthetic pairs
~/fx/synthetics.pl --timeframe=$TF

if [[ "$TF" == "3600" ]]; then
# Create synthetic timeframes
~/fx/updateTf.pl --start="5 days ago" --timeframes=7200 --available-timeframe=hour --symbols=natural
~/fx/updateTf.pl --start="5 days ago" --timeframes=10800 --available-timeframe=hour --symbols=natural
~/fx/updateTf.pl --start="5 days ago" --timeframes=14400 --available-timeframe=2hour --symbols=natural
fi
